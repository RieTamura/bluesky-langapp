{
  "expo": {
    "owner": "rietamura",
    "scheme": "blueskylearning",
    "name": "BlueskyLangApp",
    "slug": "bluesky-langapp",
    "icon": "./assets/icon.png",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.rietamura.bluelang",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "blueskylearning",
            "CFBundleURLSchemes": ["blueskylearning"]
          }
        ],
        "NSAppTransportSecurity": {
          "// NOTE": "NSAllowsArbitraryLoads is disabled by default for release builds. Use NSExceptionDomains for specific HTTP hosts.",
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "192.168.11.27": {
              "NSIncludesSubdomains": false,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.0"
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "userInterfaceStyle": "automatic",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": { "scheme": "blueskylearning" }
        }
      ]
    },
    "web": {},
    "extra": {
    "apiUrl": "http://192.168.11.27:3000",
    "blueskyClientId": "http://192.168.11.27:2584/client-metadata.json",
      "useAuthProxy": true,
      "authProxyUrl": "https://auth.expo.io/@rietamura/bluesky-langapp",
      "eas": {
        "projectId": "0bb8f270-fb24-4e9a-aa12-fcccc40bdc4c"
      }
    },
    "plugins": ["expo-secure-store", "expo-font"]
  }
}
