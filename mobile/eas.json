{
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_USE_AUTH_PROXY": "false"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_USE_AUTH_PROXY": "false"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },

      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_ATP_WORKER_URL": "https://bluelangapp.YOUR_ACCOUNT.workers.dev"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
