{
  "compilerOptions": {
    "target": "ES2020",
  "module": "Node16",
    "jsx": "react-jsx",
    "strict": true,
  "moduleResolution": "node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "react",
      "react-native",
      "jest"
    ],
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx",
    "__tests__",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
